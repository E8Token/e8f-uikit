{
  "name": "@energy8/uikit",
  "version": "0.1.1",
  "description": "Set of UI components for energy8 projects",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/E8Token/e8f-uikit",
  "scripts": {
    "build": "rimraf dist && rollup -c && tsc -d --emitDeclarationOnly --declarationDir dist",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format:check": "prettier --check --loglevel error 'src/**/*.{js,jsx,ts,tsx}'",
    "format:write": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest",
    "release": "yarn build && np"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format:write",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@rollup/plugin-typescript": "^8.2.0",
    "@rollup/plugin-url": "^6.0.0",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
    "@storybook/testing-react": "^1.2.4",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.5",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.4",
    "babel-loader": "^8.2.5",
    "babel-plugin-styled-components": "^1.11.1",
    "husky": "^8.0.1",
    "i": "^0.3.7",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "np": "^7.4.0",
    "npm": "^8.10.0",
    "prettier": "^2.6.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.39.0",
    "styled-components": "^5.2.0",
    "themeprovider-storybook": "^1.8.0",
    "ts-jest": "^26.5.1",
    "tslib": "^2.0.3",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "prop-types": "^15.8.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.0"
  },
  "dependencies": {
    "@types/styled-system": "^5.1.15",
    "prop-types": "^15.8.1",
    "styled-system": "^5.1.5"
  }
}
